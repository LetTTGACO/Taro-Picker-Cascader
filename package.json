{
  "name": "taro-picker-cascader",
  "version": "0.0.2",
  "description": "基于Taro的弹窗级联React组件",
  "templateInfo": {
    "name": "default",
    "typescript": true,
    "css": "less"
  },
  "license": "MIT",
  "author": "LetTTGACO",
  "source": "src/index.ts",
  "main": "./index.js",
  "module": "./index.esm.js",
  "types": "./index.d.ts",
  "css": "./index.css",
  "scripts": {
    "prebuild": "rimraf build",
    "build:weapp": "taro build --type weapp",
    "build:h5": "taro build --type h5",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "build:type": "tsc --project ./tsconfig.json --declaration",
    "build:rollup": "rollup --config ./rollup.config.js",
    "postbuild": "cp LICENSE build/LICENSE && cp README.md build/README.md && cp package.json build/package.json",
    "build": "npm run build:type && npm run build:rollup"
  },
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@babel/runtime": "^7.7.7",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.5",
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-babel": "^5.3.1",
    "@tarojs/cli": "3.5.2",
    "@tarojs/components": "3.5.2",
    "@tarojs/plugin-framework-react": "3.5.2",
    "@tarojs/plugin-platform-weapp": "3.5.2",
    "@tarojs/react": "3.5.2",
    "@tarojs/runtime": "3.5.2",
    "@tarojs/taro": "3.5.2",
    "@tarojs/webpack5-runner": "3.5.2",
    "@types/react": "^18.0.0",
    "@types/webpack-env": "^1.13.6",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "babel-preset-taro": "3.5.2",
    "classnames": "^2.3.1",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-taro": "3.5.2",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.8.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "postcss": "^8.4.16",
    "postcss-base64": "^0.7.1",
    "postcss-url": "^10.1.3",
    "prettier": "^2.7.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-refresh": "^0.11.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.70.1",
    "rollup-plugin-alias": "^2.2.0",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-typescript2": "^0.31.2",
    "typescript": "^4.1.0",
    "webpack": "5.69.0"
  },
  "peerDependencies": {
    "@tarojs/components": ">=3",
    "@tarojs/taro": ">=3",
    "react": ">=16.9",
    "react-dom": ">=16.9"
  }
}
